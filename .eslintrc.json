{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "off", //should be error
    "spaced-comment": "off",
    "no-console": "off", //should be warn
    "consistent-return": "off",
    "func-names": "off",
    "object-shorthand": "off",
    "no-process-exit": "off",
    "no-param-reassign": "off",
    "no-return-await": "off",
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off",
    "prefer-destructuring": [
      "off" /*error*/,
      { "object": true, "array": false }
    ],
    "no-unused-vars": [
      "off",
      /*error*/ { "argsIgnorePattern": "req|res|next|val" }
    ]
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
